{
    "name": "abr4xas/clarity-laravel",
    "description": "Easy integration of Microsoft Clarity into your Laravel application.",
    "keywords": [
        "abr4xas",
        "laravel",
        "clarity-laravel",
        "Microsoft Clarity"
    ],
    "homepage": "https://github.com/abr4xas/clarity-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Angel",
            "email": "hello@tepuilabs.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^8.17",
        "nunomaduro/collision": "^7.8",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.5",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Abr4xas\\ClarityLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abr4xas\\ClarityLaravel\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abr4xas\\ClarityLaravel\\ClarityLaravelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
